UXTerm.termName: xterm-256color
UXTerm.vt100.metaSendsEscape: true
UXTerm.vt100.backarrowKey: false
UXTerm.ttyModes: erase ^?


URxvt*depth: 24
URxvt*geometry: 100x25
URxvt.borderColor: #111113
URxvt.internalBorder: 8
/* URxvt.cursorUnderline: true */
!URxvt.cursorBlink: true

URxvt.scrollBar: false
URxvt.cursorBlink: true

! Copy and Paste
UXTerm.vt100.selectToClipboard: true
Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

URxvt.keysym.Control-Up:   \033[1;5A
URxvt.keysym.Control-Down:    \033[1;5B
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C

! Colours
! special

! made w/ http://terminal.sexy/
! Multicolor

!black
*color0:  #171717
*color8:  #737373

!red
*color1:  #FF5E5E
*color9:  #FF7878

!green
*color2:  #9CE82B
*color10: #9ACD32

!yellow
*color3:  #F0E68C
*color11: #EEE8AA

!blue
*color4:  #008AFF
*color12: #4F98FF

!magenta
*color5:  #E88CFF
*color13: #EDA6FF

!cyan
*color6:  #87CEFA
*color14: #B0E2FF

!white
*color7:  #DCDCCC
*color15: #FFFFFF

*background: #101010
*foreground: #d4d4d4

*cursorColor: darkorange2

!Fonts
UXTerm.*.faceName: xft:xos4\ TerminessTTF \Powerline:size=8:antialias=false
/* UXTerm.*.font: 7x13 */
/* URxvt.font: xft:xos4\ Terminess \Powerline:size=8:antialias=false */
/* URxv.font: 7x13 */
/* URxvt.letterSpace: -5 */
URxvt.font: xft:xos4 Terminess Powerline:pixelsize=12
/* URxvt.font: xft:xos4 Terminess Powerline:pixelsize=16 */
/* URxvt*font: xft:Droid Sans Mono for Powerline:size=10 */
URxvt*letterSpace: 0

! ------------------------------------------------------------------------------
! ROFI Color theme
! Copyright: Rasmus Steinke
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #273238, #273238, #1e2529
rofi.color-normal: #273238, #c1c1c1, #273238, #394249, #ffffff
rofi.color-active: #273238, #80cbc4, #273238, #394249, #80cbc4
rofi.color-urgent: #273238, #ff1844, #273238, #394249, #ff1844
